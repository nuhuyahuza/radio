{
    "name": "zaa-radio/booking-system",
    "description": "Zaa Radio Advertisement Booking System",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "vlucas/phpdotenv": "^5.4",
        "phpmailer/phpmailer": "^6.8",
        "dompdf/dompdf": "^2.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "migrate": "php migrate.php",
        "seed": "php seeds/seed.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
